{
  "name": "sauce-demo-testing",
  "version": "1.0.0",
  "description": "",
  "main": "cypress.config.js",
  "scripts": {
    "e2e": "cypress run",
    "e2e:parallel": "cypress-parallel -s e2e -t 2 -d cypress/e2e",
    "test:allure": "npm run e2e && npm run report:allure",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open  allure-report",
    "lint": "eslint cypress/e2e/*.cy.ts",
    "lint:fix": "eslint cypress/e2e/*.cy.ts --fix"
  },

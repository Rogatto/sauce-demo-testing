  "devDependencies": {
    "cypress": "^13.15.0",
    "cypress-parallel": "^0.14.0",
    "dotenv": "^16.4.5",
    "eslint-plugin-cypress": "^4.0.0",
    "typescript": "^5.6.3"
  }
}
